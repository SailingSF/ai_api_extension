{
  "manifest_version": 3,
  "name": "AI Image Generator",
  "version": "1.0",
  "description": "Generate images using AI",
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "favicon": "favicon.ico"
  }
}